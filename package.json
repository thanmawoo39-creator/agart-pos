  "scripts": {
    "dev": "set NODE_ENV=development && tsx server/index.ts",
    "build": "tsx script/build.ts",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --bundle --platform=node --outfile=dist/index.cjs --format=cjs --external:better-sqlite3",
    "build:all": "npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node dist/index.cjs",
    "check": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx server/lib/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx server/lib/seed.ts",
    "reset-admin": "tsx server/scripts/reset-admin.ts",
    "android:build": "npm run build:client && npx cap sync android",
    "android:open": "npx cap open android",
    "android:run": "npx cap run android",
    "cap:sync": "npx cap sync",
    "verify:build": "node -e \"const fs=require('fs'); const ok=fs.existsSync('dist/public/index.html'); console.log(ok?'✓ Build verified: dist/public/index.html exists':'✗ ERROR: dist/public/index.html not found'); process.exit(ok?0:1)\""
  },
